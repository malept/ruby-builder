{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "helpers:pinGitHubActionDigests"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchManagers": ["ruby-version"],
      "matchUpdateTypes": ["major", "minor"],
    },
  ],
  "ruby-version": {
    "managerFilePatterns": ["versions/*/.ruby-version"],
  },
}
